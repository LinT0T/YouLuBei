<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <item android:state_pressed="false">
        <shape>
        <!-- rectangle表示为矩形 -->
        <!-- 填充的颜色 -->
        <solid android:color="#2196F3" />

        <!-- 边框的颜色和粗细 -->
        <stroke
            android:width="1dp"
            android:color="#2196F3"
            />

        <!-- android:radius 圆角的半径 -->
        <corners
            android:radius="15dp"
            />
        </shape>
    </item>

</selector>